{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": "./src/",
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\|node_modules/|docs/)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "readme": "./readme.md",
    "tutorials": "./tutorials/"
    // "template": "node_modules/jsdoc/templates/jsdoc-template"
  }
}
